{
    "name": "tshafer/html",
    "description": "HTML and Form Builders for the Laravel Framework",
    "require": {
        "php": ">=7.1.3",
        "illuminate/http": "5.7.*",
        "illuminate/routing": "5.7.*",
        "illuminate/session": "5.7.*",
        "illuminate/support": "5.7.*",
        "illuminate/view": "5.7.*"
    },
    "require-dev": {
        "illuminate/database": "5.7.*",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.1"
    },
    "autoload": {
        "psr-4": {
            "Tshafer\\Html\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.7-dev"
        },
        "laravel": {
            "providers": [
                "Tshafer\\Html\\HtmlServiceProvider"
            ],
            "aliases": {
                "Form": "Tshafer\\Html\\FormFacade",
                "Html": "Tshafer\\Html\\HtmlFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
